{
  "name": "liftbridge",
  "version": "0.0.1",
  "description": "Node.js client for Liftbridge",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/paambaati/node-liftbridge",
    "type": "git"
  },
  "author": "GP <me@httgp.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc",
    "debug": "ts-node src/debug.ts",
    "format": "eslint --fix --ext .ts src/ && eslint --fix --config .eslintrc.test.json --ext .ts test/",
    "lint": "eslint --ext .ts src/",
    "lint:test": "eslint --config .eslintrc.test.json --ext .ts test/",
    "test": "NODE_ENV=test LOG_LEVEL=silent tape -r ts-node/register/transpile-only test/*.test.ts",
    "coverage": "rm -rf ./node_modules/.cache && rm -rf coverage/ && rm -rf .nyc_output/ && NODE_ENV=test LOG_LEVEL=silent nyc tape -r ts-node/register/transpile-only test/*.test.ts"
  },
  "dependencies": {
    "@sindresorhus/fnv1a": "1.2.0",
    "bluebird": "3.5.5",
    "debug": "4.1.1",
    "exponential-backoff": "2.1.1",
    "grpc": "1.23.3",
    "hyperid": "2.0.2"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.27",
    "@types/debug": "4.1.5",
    "@types/node": "12.7.5",
    "@typescript-eslint/eslint-plugin": "2.2.0",
    "@typescript-eslint/parser": "2.2.0",
    "eslint": "6.3.0",
    "eslint-config-airbnb-typescript": "4.0.1",
    "eslint-plugin-import": "2.18.2",
    "grpc-tools": "1.8.0",
    "husky": "3.0.5",
    "nyc": "14.1.1",
    "tape": "4.11.0",
    "ts-node": "8.3.0",
    "ts-protoc-gen": "0.11.0",
    "typescript": "3.6.3"
  },
  "resolutions": {
    "protobufjs": "6.8.8"
  }
}
